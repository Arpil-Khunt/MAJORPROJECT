<% layout("./layouts/boilerplate") -%>
<body>
  <h1>All Listings</h1>
  <form method="GET" action="/listings/new">
    <button>Create New Listing</button>
  </form>
  <ul>
    <% for(let listing of allListings){ %>
    <li>
      <a href="/listings/<%= listing._id %>" style="text-decoration: none"
        ><%= listing.title %></a
      >
    </li>
    <% } %>
  </ul>
</body>
